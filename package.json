{
  "name": "@node-red-contrib-themes/solarized-dark-grey",
  "version": "1.0.2",
  "description": "A dark theme for Node-RED inspired by Solarized color palette",
  "repository": {
    "type": "git",
    "url": "https://github.com/node-red-contrib-themes/solarized-dark-grey.git"
  },
  "keywords": [
    "node-red",
    "nodered",
    "solarized",
    "dark",
    "grey",
    "theme"
  ],
  "author": "Ristomatti Airo <ristomatti.airo@gmail.com>",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/node-red-contrib-themes/solarized-dark-grey/issues"
  },
  "homepage": "https://github.com/node-red-contrib-themes/solarized-dark-grey#readme",
  "scripts": {
    "init": "npm install && cd .node-red && npm install",
    "start": "node-red --userDir .node-red",
    "watch": "nodemon --ext css,js"
  },
  "devDependencies": {
    "node-red": "^2.0.6",
    "nodemon": "^2.0.13"
  },
  "node-red": {
    "plugins": {
      "solarized-dark": "theme.js"
    }
  }
}
